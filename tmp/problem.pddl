(define (problem blocks-1)
	(:domain blocksworld)
	(:objects b1 b2 b3 b4 b5)
	(:init 
		(on-table b1)
		(on-table b2)
		(on-table b3)
		(on-table b4)
		(clear b1)
		(clear b2)
		(clear b3)
		(clear b5)
		(arm-empty )
		(in-tower b4)
		(on b5 b4)
		(in-tower b5)
		(red b3)
		(red b5)
	)
	(:goal (and (forall (?x) (in-tower ?x)) (forall (?x ?y) (or (not (red ?x)) (blue ?y))) (not (on b3 b5))))
)